{
  "mcpServers": {
    "mongodb-playbg": {
      "command": "npx",
      "args": [
        "mongodb-mcp-server",
        "--connection-string",
        "REPLACE_WITH_YOUR_MONGODB_CONNECTION_STRING"
      ]
    },
    "render-playbg-dev": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-fetch",
        "https://mcp.render.com/mcp"
      ],
      "env": {
        "RENDER_API_KEY": "REPLACE_WITH_YOUR_RENDER_API_KEY"
      }
    }
  }
}